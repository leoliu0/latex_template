\newcommand{\bintree}[4]{%
\begin{picture}(65,65)(50,0)
\unitlength2mm
\setcounter{bintreea}{15}\addtocounter{bintreea}{-#4}
\thicklines
\put(20,0){\line(4,1){25}}
\put(20,0){\line(4,-1){25}}
\put(\thebintreea,0){\makebox(0,0){#1}}
\put(46,5.5){#2}
\put(46,-7.5){#3}
\end{picture}
\hspace*{6cm}
\vspace*{1.5cm}
}

\newcommand{\bintreelabels}[6]{%
