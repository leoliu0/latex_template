%% Theorem environments

% Bold headings and Italic body text.
\newtheorem{thm}{Theorem}%[section]    
  %% Counter also includes section # and is reset for the next section
%\newtheorem{thm}{Theorem}
  %% Counter is only one # which is never reset
\newtheorem{defn}{Definition}
  %% Eliminate '[thm]' if you want to count Definitions separately
\newtheorem{prop}{Proposition}
\newtheorem{cor}{Corollary}
\newtheorem{lemma}{Lemma}

\theoremstyle{definition}  
  %% \theoremstyle{Definition} is the default, with Italic body text.
\newtheorem{example}{Example}
\newtheorem{remark}{Remark}
\newtheorem{conj}{Conjecture}
\newtheorem{assumption}{Assumption}
%
\newtheorem{problem}{Problem}
\newtheorem{case}{Case}

